{
  "name": "server",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "start": "SET NODE_ENV=production&& node dist/index.js",
    "start:fordocker": "node dist/index.js",
    "postinstall": "prisma generate",
    "dev": "SET NODE_ENV=development&&  ts-node ts-node --files src/index.ts",
    "dev:nodemon": "SET NODE_ENV=development&& nodemon --watch src --exec ts-node --files src/index.ts",
    "prisma:generate": "dotenv -e .env.development -- npx prisma generate",
    "prisma:create": "dotenv -e .env.development -- npx prisma migrate dev --create-only",
    "prisma:migrate": "dotenv -e .env.development -- npx prisma migrate dev",
    "prisma:pull": "dotenv -e .env.development -- npx prisma db pull",
    "prisma:migration-status": "dotenv -e .env.development -- npx prisma migrate status",
    "seed": "dotenv -e .env.development -- ts-node prisma/seed.ts", 
    "initial_seed": "dotenv -e .env.development -- ts-node prisma/initialSeed.ts"
  },
  "author": "Zeeshan Ali",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^22.5.5",
    "dotenv-cli": "^7.4.2",
    "prisma": "^5.19.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@prisma/client": "^5.19.0",
    "@types/lodash": "^4.17.7",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "email-existence": "^0.1.6",
    "express": "^4.19.2",
    "global": "^4.4.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "winston": "^3.14.2",
    "winston-daily-rotate-file": "^5.0.0"
  }
}
